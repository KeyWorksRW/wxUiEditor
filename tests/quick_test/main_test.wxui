<?xml version="1.0"?>
<wxUiEditorData
  data_version="16">
  <node
    class="Project"
    art_directory="../art"
    code_preference="Ruby"
    base_directory="cpp"
    cpp_line_length="125"
    wxWidgets_version="3.2"
    python_output_folder="python"
    disable_rubo_cop="1"
    ruby_output_folder="ruby"
    xrc_directory="xrc">
    <node
      class="wxFrame"
      class_name="MainFrame"
      title="Tests"
      base_file="main_frame"
      use_derived_class="0"
      python_file="py_main"
      python_import_list="test_dlg.py;wizard.py"
      relative_require_list="test_dlg.rb;wizard.rb"
      ruby_file="rb_main"
      xrc_file="xrc_main"
      minimum_size="300,-1d">
      <node
        class="wxMenuBar"
        var_name="menubar">
        <node
          class="wxMenu"
          label="&amp;File"
          var_name="menu_file">
          <node
            class="wxMenuItem"
            help="Quit this program"
            id="wxID_EXIT"
            label="&amp;Quit"
            stock_id="wxID_EXIT"
            wxEVT_MENU="OnQuit[python:on_quit][ruby:on_quit]" />
        </node>
        <node
          class="wxMenu"
          class_access="none"
          label="Tests"
          var_name="menu_dialogs">
          <node
            class="wxMenuItem"
            label="Test Dialog..."
            var_name="menu_item_test_dlg"
            wxEVT_MENU="OnTestDialog[python:on_test_dlg][ruby:on_test_dlg]" />
          <node
            class="wxMenuItem"
            label="Wizard..."
            var_name="menu_item_test_wizard"
            wxEVT_MENU="OnWizard[python:on_wizard][ruby:on_wizard]" />
        </node>
      </node>
      <node
        class="wxBoxSizer"
        orientation="wxVERTICAL">
        <node
          class="wxPanel"
          window_style="wxTAB_TRAVERSAL"
          borders=""
          flags="wxEXPAND"
          proportion="1">
          <node
            class="wxBoxSizer"
            orientation="wxVERTICAL"
            var_name="panel_sizer">
            <node
              class="wxTextCtrl"
              style="wxTE_MULTILINE"
              var_name="text_ctrl"
              minimum_size="200,100d"
              flags="wxEXPAND"
              proportion="1" />
          </node>
        </node>
      </node>
    </node>
  </node>
</wxUiEditorData>
