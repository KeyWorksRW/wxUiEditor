<?xml version="1.0"?>
<wxUiEditorData
  data_version="15">
  <node
    class="Project"
    code_preference="Ruby"
    cpp_line_length="125"
    wxWidgets_version="3.2"
    python_output_folder="python"
    disable_rubo_cop="1"
    ruby_output_folder="ruby"
    xrc_directory="xrc">
    <node
      class="folder"
      code_preference="Ruby"
      label="Ruby"
      folder_python_output_folder="python"
      folder_ruby_output_folder="ruby"
      folder_xrc_directory="xrc">
      <node
        class="wxFrame"
        class_name="MainFrame"
        title="Tests"
        python_file="py_main"
        python_import_list="test_dlg.py"
        relative_require_list="test_dlg.rb"
        ruby_file="rb_main"
        xrc_file="xrc_main"
        minimum_size="300,-1d">
        <node
          class="wxMenuBar"
          var_name="menubar">
          <node
            class="wxMenu"
            label="&amp;File"
            var_name="menu_file">
            <node
              class="wxMenuItem"
              help="Quit this program"
              id="wxID_EXIT"
              label="&amp;Quit"
              stock_id="wxID_EXIT"
              wxEVT_MENU="OnMenuSelection[python:on_quit][ruby:on_quit]" />
          </node>
          <node
            class="wxMenu"
            class_access="none"
            label="Tests"
            var_name="menu_dialogs">
            <node
              class="wxMenuItem"
              label="Test Dialog"
              var_name="menu_item_test_dlg"
              wxEVT_MENU="OnMenuSelection[python:OnTestDlg][ruby:on_test_dlg]" />
          </node>
        </node>
        <node
          class="wxBoxSizer"
          orientation="wxVERTICAL">
          <node
            class="wxPanel"
            window_style="wxTAB_TRAVERSAL"
            borders=""
            flags="wxEXPAND"
            proportion="1">
            <node
              class="wxBoxSizer"
              orientation="wxVERTICAL"
              var_name="panel_sizer">
              <node
                class="wxTextCtrl"
                style="wxTE_MULTILINE"
                var_name="text_ctrl"
                minimum_size="200,100d"
                flags="wxEXPAND"
                proportion="1" />
            </node>
          </node>
        </node>
      </node>
      <node
        class="wxDialog"
        class_name="TestDialog"
        title="Test Dialog"
        base_file="rubytest"
        derived_class_name="TestDialogDerived"
        derived_file="testdialog_derived"
        python_file="test_dlg"
        ruby_file="test_dlg"
        xrc_file="test_dlg"
        wxEVT_INIT_DIALOG="OnInit[python:OnInit][ruby:on_init]">
        <node
          class="wxBoxSizer"
          orientation="wxVERTICAL"
          var_name="dlg_sizer"
          flags="wxEXPAND">
          <node
            class="wxBoxSizer"
            var_name="box_sizer_2">
            <node
              class="wxStaticText"
              var_name="static_text"
              alignment="wxALIGN_CENTER_VERTICAL" />
            <node
              class="wxTextCtrl"
              var_name="text_ctrl" />
            <node
              class="wxCheckBox"
              var_name="check_box"
              alignment="wxALIGN_CENTER_VERTICAL" />
          </node>
        </node>
      </node>
    </node>
  </node>
</wxUiEditorData>
