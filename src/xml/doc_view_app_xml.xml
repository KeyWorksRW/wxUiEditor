inline const char* doc_view_app_xml = R"===(<?xml version="1.0"?>
<!DOCTYPE GeneratorDefinitions SYSTEM "gen.dtd">
<GeneratorDefinitions>
	<gen class="DocViewApp" image="wxFrame" type="DocViewApp">
